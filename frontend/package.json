    "eslint-plugin-react-hooks": "^4.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sendbird-uikit": "^2.6.0",
    "tailwindcss": "^3.0.5",
    "typescript": "4.5.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/test/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "yarn lint"
    ]
  }
}
